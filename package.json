{
  "name": "reactjs-films-homework",
  "version": "1.0.0",
  "repository": "git@github.com:d90375/reactjs-films-homework.git",
  "author": "d90375 <d90375@gmail.com>",
  "license": "MIT",
  "browserslist": [
    ">0.25%",
    "not dead",
    "not ie <= 11"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development cross-env node ./server/server.js",
    "prod": "cross-env NODE_ENV=production npm run clean && webpack-dev-server --mode production --config webpack.production.config.js --open",
    "build": "cross-env NODE_ENV=production npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "prettier": "prettier ./src/**/**.js --write",
    "lint": "eslint ./src/**/**.js",
    "test:jest": "jest",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rimraf build"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "./src/": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/index.jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.10.4",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.10",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.1.0",
    "jest-cli": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "object-hash": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@hot-loader/react-dom": "npm:@hot-loader/react-dom",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.1",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^8.2.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm run lint && npm test"
    }
  }
}
